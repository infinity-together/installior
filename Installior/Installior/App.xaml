<Application x:Class="Installior.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Installior"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Source/ResourceDictionaries/TextDictionary.xaml" />
                <ResourceDictionary Source="Source/ResourceDictionaries/ButtonDictionary.xaml" />
            </ResourceDictionary.MergedDictionaries>

            <FontFamily x:Key="Inter">pack://application:,,,/Source/Fonts/#Inter</FontFamily>
        </ResourceDictionary>
    </Application.Resources>
</Application>
